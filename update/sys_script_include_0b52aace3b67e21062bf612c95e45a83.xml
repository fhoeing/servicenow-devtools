        <api_name>x_snc_devtools.GetSysIdsFromString</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns an array of Sys IDs that could be extracted from the given string.</description>
        <mobile_callable>false</mobile_callable>
        <name>GetSysIdsFromString</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function GetSysIdsFromString(strInput)
{
	strInput = GetStringValue(strInput);

	var aSysIds = [];
	
	var regex = /\b[a-f0-9]{32}\b/g;
	var matches = strInput.match(regex);
	if (matches) 
	{
		aSysIds = matches;
	}
	return aSysIds;
}
]]></script>
