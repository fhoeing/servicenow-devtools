        <api_name>x_snc_devtools.IsDataRecord</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns true if the given GlideRecord object or Sys ID represents a data record - i.e. does NOT represent a code record.</description>
        <mobile_callable>false</mobile_callable>
        <name>IsDataRecord</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function IsDataRecord(vRecordOrSysId)
{
	if (IsValidRecord(vRecordOrSysId) == false && IsValidSysId(vRecordOrSysId) == false)
	{
		return false;
	}
	return (IsCodeRecord(vRecordOrSysId) == false);
}]]></script>
