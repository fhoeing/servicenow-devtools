        <action_name>devtools_updateset_pack</action_name>
        <active>true</active>
        <client>false</client>
        <client_script_v2><![CDATA[function onClick(g_form) {

}]]></client_script_v2>
        <comments>Packs the completed Update Set into a new Record Transporter Package.</comments>
        <condition>IsActive() &amp;&amp; (AppGetProperty('ui.updateset_pack_button') == true) &amp;&amp; (current.state == 'complete') &amp;&amp; (IsValidSysId(current.x_snc_devtools_transporter_package_application)) &amp;&amp; (IsValidSysId(current.x_snc_devtools_transporter_package) == false)</condition>
        <form_action>true</form_action>
        <form_button>false</form_button>
        <form_button_v2>false</form_button_v2>
