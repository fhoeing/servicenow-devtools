        <list_style/>
        <messages/>
        <name>Pack Update Set into Record Transporter</name>
        <onclick/>
        <order>99999</order>
        <script><![CDATA[var grApp = GetAppRecord(current.x_snc_devtools_transporter_package_application);
if (IsValidRecord(grApp) == false)
{
	gs.addErrorMessage('Could not find specified application! Failed to pack this Update Set!');
}
else
{
	var strTransporterPackageSysId = RecordTransporterPackUpdateSetIntoApp(
		grApp,
		current);
	if (IsValidSysId(strTransporterPackageSysId) == true)
	{
		gs.addInfoMessage(
			StringFormat(
				'The Update Set "{0}" has been packed into a new Record Transporter Package in Application "{1}".',
				current.name,
				grApp.name)
			);
	}
	else
	{
		gs.addErrorMessage(
			StringFormat(
				'Failed to pack Update Set "{0}" into Application "{1}".',
				current.name,
				grApp.name)
			);
	}
}
RedirectToRecord(current);]]></script>
