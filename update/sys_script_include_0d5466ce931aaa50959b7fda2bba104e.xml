        <api_name>x_snc_devtools.TimeAddHours</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Adds (or substracts) a given number of hours to (or from) a given time value (in milliseconds).</description>
        <mobile_callable>false</mobile_callable>
        <name>TimeAddHours</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function TimeAddHours(nTime, nHours)
{
	nTime = GetIntegerValue(nTime);
	nHours = GetIntegerValue(nHours);
	nTime += nHours * 1000 * 60 * 60;
	return nTime;
}
]]></script>
