    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_devtools.HtmlRenderTableOfContents</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Adds the HTML and script footprint to render a table of contents into an existing page.</description>
        <mobile_callable>false</mobile_callable>
        <name>HtmlRenderTableOfContents</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function HtmlRenderTableOfContents()
{
	var strHtml = '';

	strHtml += '<div id="toc"></div>';
	
	strHtml += HtmlRenderScript(
		"document.addEventListener('DOMContentLoaded', function () { MakeTableOfContents('toc',2,true); });");
	
	return strHtml;
}]]></script>
