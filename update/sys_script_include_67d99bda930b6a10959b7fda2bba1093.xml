        <api_name>x_snc_devtools.HtmlRenderTextEmphasized</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Renders an emphasized text span.</description>
        <mobile_callable>false</mobile_callable>
        <name>HtmlRenderTextEmphasized</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function HtmlRenderTextEmphasized(strText)
{
	strText = GetStringValue(strText);
	if (strText == '')
	{
		return '';
	}
	var strHtml = '';
	strHtml += '<span style="font-weight: bold; color: red">';
			strHtml += strText;
	strHtml += '</span>';
	return strHtml;
}]]></script>
