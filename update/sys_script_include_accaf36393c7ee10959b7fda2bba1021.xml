    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_devtools.GetDefaultTechnicalUser</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns the GlideRecord object repreenting the default technical user if it is possible to identify one.</description>
        <mobile_callable>false</mobile_callable>
        <name>GetDefaultTechnicalUser</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function GetDefaultTechnicalUser()
{
	var grUser = GetUserRecord('technical.default');
	if (IsValidRecord(grUser))
	{
		return grUser;
	}

	var grUser = GetUserRecord('admin');
	if (IsValidRecord(grUser))
	{
		return grUser;
	}
	
	return false; 
}]]></script>
