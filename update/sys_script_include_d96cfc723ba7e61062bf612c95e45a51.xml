    <sys_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <active>true</active>
        <api_name>x_snc_devtools.GetStudioPath</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns the URL path to the Studio application - using the given application.</description>
        <mobile_callable>false</mobile_callable>
        <name>GetStudioPath</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function GetStudioPath(vApp)
{
	var strAppSysId = '';
	var grApp = GetAppRecord(vApp);
	if (IsValidRecord(grApp) == true)
	{
		strAppSysId = GetStringValue(grApp.sys_id);
	}
	else
	{
		if (IsValidSysId(vApp) == true)
		{
			strAppSysId = vApp;
		}
	}
	var strAppParam = '';
	if (IsValidSysId(strAppSysId) == true)
	{
		strAppParam = '?sysparm_transaction_scope=' + strAppSysId;
	}
	return '$studio.do' + strAppParam;
}]]></script>
