<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ui_script">
    <sys_ui_script action="INSERT_OR_UPDATE">
        <active>true</active>
        <description>Returns the url path of a specific record.</description>
        <global>true</global>
        <ignore_in_now_experience>false</ignore_in_now_experience>
        <name>x_snc_devtools.GetRecordPath</name>
        <script><![CDATA[function GetRecordPath(strTable,strSysId,strView,params)
{
	var strUri = GetRecordUri(strTable,strSysId,strView,params);
	if (strUri == false)
	{
		return false;
	}
	var strPath = 'nav_to.do?uri=' +encodeURIComponent(strUri);
	return strPath;
}]]></script>
        <script_name>GetRecordPath</script_name>
