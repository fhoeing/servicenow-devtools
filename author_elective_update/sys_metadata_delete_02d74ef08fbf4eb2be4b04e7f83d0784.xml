        <api_name>x_snc_devtools.GetInstanceRecord</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns the record representing the given Instance based on its Sys ID or the Instance name (as specified in the Instance record). </description>
        <mobile_callable>false</mobile_callable>
        <name>GetInstanceRecord</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function GetInstanceRecord(vInstance)
{
	var GetRecord = x_snc_devtools.GetRecord;
	var grInstance = GetRecord('x_snc_deployer_instance',vInstance);
	if (IsValidRecord(grInstance) == true)
	{
		return grInstance;
	}
	grInstance = new GlideRecord('x_snc_deployer_instance');
	return false;
}]]></script>
